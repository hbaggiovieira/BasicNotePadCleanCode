<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DefaultToolbar">
        <attr name="title" format="string" />
        <attr name="progress" format="integer" />
        <attr name="fullProgressValue" format="integer" />
        <attr name="hasProgress" format="boolean" />
        <attr name="hasCloseButton" format="boolean" />
        <attr name="hasBackButton" format="boolean" />
        <attr name="rightIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="EditTextWithErrorState">
        <attr name="state_has_error" format="boolean" />
    </declare-styleable>

    <declare-styleable name="CardConfirmation">
        <attr name="icon" format="reference" />
        <attr name="titleText" format="string" />
        <attr name="subtitleText" format="string" />
        <attr name="hasSubtitle" format="boolean" />
    </declare-styleable>

</resources>